        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/con-tax/properties/cp#hasDepth -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#hasDepth">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/con-tax#property_OP"/>
        <rdfs:comment xml:lang="en">Quantitative property to indicate the depth of a spatial building zone or a construction component. It corresponds with the horizontal distance different from the width. E.g. the depth (or thickness) of a window.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">has depth</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Element"/>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Zone"/>
