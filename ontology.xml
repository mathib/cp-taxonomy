    <!-- https://w3id.org/con-tax/properties/cp#assessmentFromInspection -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#assessmentFromInspection">
        <rdfs:range rdf:resource="https://w3id.org/dot#Inspection"/>
        <rdfs:comment xml:lang="en">Additional property to connect an assessment node (between an assessed object and the assessed value) to the related inspection. It can be considered metadata for the assessment. Property inspired by Monumentenwacht reports and the DOT extension DASB.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">assessment from inspection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/con-tax/properties/cp#countedVisitors -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#countedVisitors">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/con-tax#property_OP"/>
        <rdfs:comment xml:lang="en">Quantitative property to indicate the number of counted visitors of a building zone. The starting and ending of the counting is indicated with a metadata property prov:startedAtTime and prov:endedAtTime. Property definition inspired by interviews with built heritage stakeholders.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">counted visitors</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Zone"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/con-tax/properties/cp#createdBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#createdBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/con-tax/properties/cp#relatedPersonOrOrganization"/>
        <rdfs:comment xml:lang="en">Property to indicate that a construction task is defined by a certain person or organization.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">created by</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/cto#Task"/>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/cto#TaskMethod"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2006/vcard/ns#Individual"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2006/vcard/ns#Organization"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/con-tax/properties/cp#hasArea -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#hasArea">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/con-tax#property_OP"/>
        <rdfs:comment xml:lang="en">Quantitative property to indicate the area of a building interface (connection between building zones, between a building zone and a building element or between two building elements) or a spatial building zone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">has area</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Interface"/>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Zone"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/con-tax/properties/cp#hasAssessment -->


    <owl:ObjectProperty rdf:about="https://w3id.org/con-tax/properties/cp#hasAssessment">
        <rdfs:range rdf:resource="https://w3id.org/con-tax/properties/cp#Assessment"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Element"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Zone"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/dot#Damage"/>
        <rdfs:comment xml:lang="en">A property to connect an assessed object (construction component, spatial zone or damage) to an assessment node. The assessment node can be connected to (1) its assessment score(s) using (subproperties of) cp:assessmentScore and (2) the inspection context of the said assessment using cp:assessmentFromInspection. Property inspired by Monumentenwacht reports and the DOT extension DASB.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">has assessment</rdfs:label>
    </owl:ObjectProperty>
    


