        <rdfs:subPropertyOf rdf:resource="https://w3id.org/con-tax#property_OP"/>
        <rdfs:comment xml:lang="en">Quantitative property to indicate the length of a crack (damage element). It corresponds with the distance along the crack. This property is inspired by the cdo:crackLengthproperty in the Concrete Damage Ontology (CDO) extension of DOT.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">has crack length</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/dot#DamageElement"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:ObjectProperty>
    


