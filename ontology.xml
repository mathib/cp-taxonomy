        <rdfs:label xml:lang="en">has material part</rdfs:label>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/bot#Element"/>
        <con-tax:domainOrSubjectIncludes rdf:resource="https://w3id.org/con-tax#ConstructionComponent"/>
        <con-tax:rangeOrObjectIncludes rdf:resource="https://w3id.org/con-tax#Material"/>
    </owl:ObjectProperty>
    


