        <rdfs:comment xml:lang="en">Quantitative property to indicate the ceiling area of a spatial building zone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/con-tax/properties/cp"/>
        <rdfs:label xml:lang="en">has floor area</rdfs:label>
